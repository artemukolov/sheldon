{
    "name": "artemukolov/sheldon",
    "type": "library",
    "description": "Sheldon ORM: php library",
    "keywords": [
        "Ukolov", "sheldon", "ORM", "db"
    ],
    "minimum-stability": "dev",
    "homepage": "https://github.com/artemukolov/sheldon",
    "license": "MIT",
    "authors": [
        {
            "name": "Artem Ukolov",
            "email": "artem@ukolov.info",
            "homepage": "http://ukolov.info",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Ukolov\\Sheldon\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ukolov\\Sheldon\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
